{
  "expo": {
    "name": "UniClaim",
    "slug": "UniClaim",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/uniclaimlogo.png",
    "scheme": "UniClaim",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/uniclaimlogo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "softwareKeyboardLayoutMode": "resize",
      "package": "com.bluerat12.UniClaim"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/uniclaimlogo.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/uniclaimlogo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/uniclaimlogo.png",
          "color": "#ffffff",
          "defaultChannel": "default"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "9ee38d41-83ad-4306-bff2-97c396db3856"
      }
    },
    "env": {
      "NODE_ENV": "production",
      "EXPO_PUBLIC_APP_ENV": "production"
    },
    "owner": "bluerat12"
  }
}